{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "Credit Card Customer",
	"type": "object",
	"properties": {
		"name": {
			"type": "string"
		},
		"number": {
			"type": "string"
		},
		"expiration_year": {
			"type": "integer"
		},
		"expiration_month": {
			"type": "integer"
		},
		"cvv": {
			"type": "string"
		}
	},
	"required": ["name", "number", "expiration_year", "expiration_month", "cvv"],
	"additionalProperties": false
}
